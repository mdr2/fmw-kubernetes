<AuthenticationPolicies>
   <AuthenticationPolicy>
        <name>OAA_MFA-Policy</name>
        <description>OAA Authentication Policy</description>
        <applicationDomainName>IAM Suite</applicationDomainName>
        <authenticationSchemeName>OAA-MFA-Scheme</authenticationSchemeName>
        <authnLevel>2</authnLevel>
        <usingDefaultAuthnScheme>false</usingDefaultAuthnScheme>
   </AuthenticationPolicy>
</AuthenticationPolicies>
